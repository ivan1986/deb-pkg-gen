{
  "name": "ivan1986/deb-pkg",
  "license": "MIT",
  "type": "project",
  "description": "Generate deb packages for repository",
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.0"
    },
    "preferred-install": "source"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jordisala1991/SonataUserBundle"
    }
  ],
  "require": {
    "php": "~7.0",
    "ext-xml": "*",
    "ext-gnupg": "*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.5",
    "doctrine/doctrine-migrations-bundle": "~1",
    "friendsofsymfony/rest-bundle": "~2",
    "jms/di-extra-bundle": "~1",
    "jms/security-extra-bundle": "~1",
    "jms/serializer-bundle": "~1",
    "lunetics/locale-bundle": "~2.0",
    "nelmio/api-doc-bundle": "~2.0",
    "incenteev/composer-parameter-handler": "~2",
    "sensio/buzz-bundle": "~1",
    "sensio/distribution-bundle": "~5",
    "sensio/framework-extra-bundle": "~3",
    "sonata-project/doctrine-orm-admin-bundle": "~3.0",
    "sonata-project/easy-extends-bundle": "~2.1",
    "sonata-project/user-bundle": "dev-feature/fos_user_2",
    "sonata-project/exporter": "~1.7",
    "stof/doctrine-extensions-bundle": "~1.2",
    "symfony/assetic-bundle": "~2.8",
    "symfony/monolog-bundle": "~3.0",
    "symfony/swiftmailer-bundle": "~2.5",
    "symfony/symfony": "~3.0",
    "twig/extensions": "~1.4",
    "mopa/bootstrap-bundle": "~3.1",
    "twbs/bootstrap-sass": "~3.3",
    "united-prototype/php-ga": "dev-composer",
    "white-october/pagerfanta-bundle": "~1",
    "elao/error-notifier-bundle": "~1",
    "endroid/google-analytics-bundle": "~1",
    "meinfernbus/google-bundle": "~2.3",
    "guzzlehttp/guzzle": "^6.2",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "cweagans/composer-patches": "~1"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "friendsofphp/php-cs-fixer": "~2.1",
    "symfony/phpunit-bridge": "^3",
    "codeception/codeception": "~2.2",
    "codeception/c3": "~2",
    "ivan1986/dev-container": "~0.3"
  },
  "scripts": {
    "symfony": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony"
    ],
    "post-update-cmd": [
      "@symfony"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "patches": {
      "sonata-project/core-bundle": {
        "Symfony 3.3 fix": "https://patch-diff.githubusercontent.com/raw/sonata-project/SonataCoreBundle/pull/396.diff"
      }
    }
  }
}
